{
	"name": "express-react-router",
	"version": "2.0.0-rc1",
	"description": "Express middleware for react-router",
	"author": "Nick Heyn <nick@heyn.io>",
	"main": "./lib/index.js",
	"repository" : {
		"type" : "git",
		"url" : "https://github.com/nheyn/express-react-router"
	},
	"dependencies": {
		"react": "^0.14.6",
		"react-dom": "^0.14.6",
		"react-router": "^2.0.0-rc5",
		"express": "4"
	},
	"devDependencies": {
		"babel-cli": "6",
		"babel-preset-es2015": "6",
		"babel-preset-react": "6",
		"flow-bin": "^0.20.0"
	},
	"scripts": {
		"flow": "(cd src && ../node_modules/.bin/flow)",
		"flow:stop": "(cd src && ../node_modules/.bin/flow stop)",
		"check": "npm run flow",
		"build": "npm run check && babel ./src/ -d ./lib/",
		"prepublish": "npm run build",
		"clean": "rm -rf ./lib/ && npm flow:stop"
	},
	"engines": {
		"node": "v5"
	},
	"license": "MIT"
}
