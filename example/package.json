{
	"name": "express-react-router-example",
	"version": "2.0.0-rc1",
	"description": "A simple website that uses express-react-router",
	"author": "Nick Heyn <nick@heyn.io>",
	"dependencies": {
		"react": "^0.14.6",
		"react-dom": "^0.14.6",
		"react-router": "^2.0.0-rc5",
		"express": "4",
		"babel-polyfill": "6",
		"babel-cli": "6",
		"babel-preset-es2015": "6",
		"babel-preset-stage-0": "6",
		"babel-preset-react": "6",
		"browserify": "13",
		"express-react-router": "file:../express-react-router/"
	},
	"scripts": {
		"build:babel": "babel ./src -d ./lib",
		"build:browserify": "node browserify.js",
		"build": "npm run build:babel && npm run build:browserify",
		"start": "npm run build && node ./lib/server.js"
	},
	"engines": {
		"node": "v5"
	}
}
